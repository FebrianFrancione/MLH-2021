CREATE TABLE `mysql`.`users` (
   `user_id` INT NOT NULL AUTO_INCREMENT,
   `username` VARCHAR(45) NULL,
   `first_name` VARCHAR(45) NULL,
   `last_name` VARCHAR(45) NULL,
   `password` VARCHAR(45) NULL,
   `sports` VARCHAR(45) NULL,
   PRIMARY KEY (`user_id`));


DROP table user;


CREATE TABLE `mysql`.`event` (
  `event_id` INT NOT NULL AUTO_INCREMENT,
  `event_name` VARCHAR(45) NULL,
  `event_date_start` DATE NULL,
  `event_date_end` DATE NULL,
  `event_users` VARCHAR(45) NULL,
  PRIMARY KEY (`event_id`)
  FOREIGN KEY (event_users)
  REFERENCES users(user_id) ON UPDATE CASCADE ON DELETE RESTRICT
  );

  CREATE TABLE `albums_db`.`event` (
    `event_id` INT NOT NULL AUTO_INCREMENT,
    `event_name` VARCHAR(45) NULL,
    `event_date_start` DATE NULL,
    `event_date_end` DATE NULL,
    `event_users` VARCHAR(45) NULL,
    PRIMARY KEY (`event_id`));


CREATE TABLE `albums_db`.`userevents` (
  `event_id` INT NULL,
  `user_id` INT NULL,
  INDEX `event_id_idx` (`event_id` ASC) VISIBLE,
  INDEX `user_id_idx` (`user_id` ASC) VISIBLE,
  CONSTRAINT `event_id`
    FOREIGN KEY (`event_id`)
    REFERENCES `albums_db`.`event` (`event_id`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `user_id`
    FOREIGN KEY (`user_id`)
    REFERENCES `albums_db`.`users` (`user_id`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION);

